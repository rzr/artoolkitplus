#############################################################################
# Makefile for building: sample
# Generated by qmake (1.07a) (Qt 3.3.7) on: Sat Jan 20 01:24:36 2007
# Project:  sample.pro
# Template: subdirs
# Command: $(QMAKE) -o Makefile sample.pro
#############################################################################

MAKEFILE =	Makefile
QMAKE    =	qmake
DEL_FILE =    rm -f
CHK_DIR_EXISTS= test -d
MKDIR    = mkdir -p
INSTALL_FILE= 
INSTALL_DIR = 
SUBTARGETS =	 \
		sub-simple \
		sub-multi

first: all

all: Makefile $(SUBTARGETS)

simple/$(MAKEFILE): 
	@$(CHK_DIR_EXISTS) "simple" || $(MKDIR) "simple"
	cd simple && $(QMAKE) simple.pro -o $(MAKEFILE)
sub-simple: simple/$(MAKEFILE) FORCE
	cd simple && $(MAKE) -f $(MAKEFILE)

multi/$(MAKEFILE): 
	@$(CHK_DIR_EXISTS) "multi" || $(MKDIR) "multi"
	cd multi && $(QMAKE) multi.pro -o $(MAKEFILE)
sub-multi: multi/$(MAKEFILE) FORCE
	cd multi && $(MAKE) -f $(MAKEFILE)

Makefile: sample.pro  /usr/share/qt3/mkspecs/default/qmake.conf 
	$(QMAKE) -o Makefile sample.pro
qmake: qmake_all
	@$(QMAKE) -o Makefile sample.pro

all: $(SUBTARGETS)
qmake_all: simple/$(MAKEFILE) multi/$(MAKEFILE)
	 ( [ -d simple ] && cd simple ; grep "^qmake_all:" $(MAKEFILE) && $(MAKE) -f $(MAKEFILE) qmake_all; ) || true
	 ( [ -d multi ] && cd multi ; grep "^qmake_all:" $(MAKEFILE) && $(MAKE) -f $(MAKEFILE) qmake_all; ) || true
clean uicables mocables uiclean mocclean lexclean yaccclean : qmake_all FORCE
	 ( [ -d simple ] && cd simple ; $(MAKE) -f $(MAKEFILE) $@; ) || true
	 ( [ -d multi ] && cd multi ; $(MAKE) -f $(MAKEFILE) $@; ) || true
uninstall_subdirs: qmake_all FORCE
	 ( [ -d simple ] && cd simple ; $(MAKE) -f $(MAKEFILE) uninstall; ) || true
	 ( [ -d multi ] && cd multi ; $(MAKE) -f $(MAKEFILE) uninstall; ) || true
install_subdirs: qmake_all FORCE
	 ( [ -d simple ] && cd simple ; $(MAKE) -f $(MAKEFILE) install; ) || true
	 ( [ -d multi ] && cd multi ; $(MAKE) -f $(MAKEFILE) install; ) || true
distclean: qmake_all FORCE
	 ( [ -d simple ] && cd simple ; $(MAKE) -f $(MAKEFILE) $@; $(DEL_FILE) $(MAKEFILE); ) || true
	 ( [ -d multi ] && cd multi ; $(MAKE) -f $(MAKEFILE) $@; $(DEL_FILE) $(MAKEFILE); ) || true

install:  install_subdirs

uninstall:  uninstall_subdirs

FORCE:

