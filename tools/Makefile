MAKEFILE =	Makefile
QMAKE    =	qmake
DEL_FILE =    rm -f
CHK_DIR_EXISTS= test -d
MKDIR    = mkdir -p
INSTALL_FILE= 
INSTALL_DIR = 
SUBTARGETS =	 \
		sub-IdPatGen

first: all

all: Makefile $(SUBTARGETS)

IdPatGen/$(MAKEFILE): 
	@$(CHK_DIR_EXISTS) "IdPatGen" || $(MKDIR) "IdPatGen"
	cd IdPatGen && $(QMAKE) IdPatGen.pro -o $(MAKEFILE)
sub-IdPatGen: IdPatGen/$(MAKEFILE) FORCE
	cd IdPatGen && $(MAKE) -f $(MAKEFILE)

Makefile: tools.pro  /usr/share/qt3/mkspecs/default/qmake.conf 
	$(QMAKE) -o Makefile tools.pro
qmake: qmake_all
	@$(QMAKE) -o Makefile tools.pro

all: $(SUBTARGETS)
qmake_all: IdPatGen/$(MAKEFILE)
	 ( [ -d IdPatGen ] && cd IdPatGen ; grep "^qmake_all:" $(MAKEFILE) && $(MAKE) -f $(MAKEFILE) qmake_all; ) || true
clean uicables mocables uiclean mocclean lexclean yaccclean : qmake_all FORCE
	 ( [ -d IdPatGen ] && cd IdPatGen ; $(MAKE) -f $(MAKEFILE) $@; ) || true
uninstall_subdirs: qmake_all FORCE
	 ( [ -d IdPatGen ] && cd IdPatGen ; $(MAKE) -f $(MAKEFILE) uninstall; ) || true
install_subdirs: qmake_all FORCE
	 ( [ -d IdPatGen ] && cd IdPatGen ; $(MAKE) -f $(MAKEFILE) install; ) || true
distclean: qmake_all FORCE
	 ( [ -d IdPatGen ] && cd IdPatGen ; $(MAKE) -f $(MAKEFILE) $@; $(DEL_FILE) $(MAKEFILE); ) || true

install:  install_subdirs

uninstall:  uninstall_subdirs

FORCE:

